class Solution {
public:
    bool ispal(string str){
        int i=0;
        int j=str.size()-1;
        while(i<=j){
            if(str[i]!=str[j])return false;
            i++;
            j--;
        }
        return true;
    }
    bool isPalindrome(int x) {
        string str=to_string(x);
        return ispal(str);

        
    }
};
